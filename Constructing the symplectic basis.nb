Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "Verify", " ", "that", " ", "we", " ", "can", " ", "find", " ", "the", 
     " ", "diagonalising", " ", "symplectic", " ", "for", " ", "complex", " ",
      "and", " ", "indefinite"}], ",", " ", 
    RowBox[{"symmetric", " ", "matrices"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "Since", " ", "not", " ", "all", " ", "such", " ", "matrices", " ", 
     "admit", " ", "symplectic", " ", "diagonalisation"}], ",", " ", 
    RowBox[{
    "we", " ", "intentionally", " ", "design", " ", "one", " ", "that", " ", 
     RowBox[{"does", "."}]}]}], "*)"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"Number", " ", "of", " ", "modes"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"d", "=", "5"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Generate", " ", "a", " ", "random", " ", "symplectic"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"q", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"RandomReal", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "1"}], ",", "1"}], "}"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"2", "d"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"2", "d"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"S0", "=", 
     RowBox[{"MatrixExp", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"q", "+", 
         RowBox[{"ConjugateTranspose", "[", "q", "]"}]}], ")"}], ".", 
       RowBox[{"\[CapitalOmega]", "[", "d", "]"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   "\"\<Complex, symmetric matrix that, by explicit construction, can be \
