   RowBox[{
    RowBox[{"Eigenvalues", "[", 
     RowBox[{"I", " ", 
      RowBox[{
       RowBox[{"\[CapitalOmega]", "[", "d", "]"}], ".", "V"}]}], "]"}], "//", 
    "Chop"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "The", " ", "extra", " ", "arguments", " ", "in", " ", 
      "FindDiagonalisingSymplectic", " ", "tell", " ", "us", " ", "that", " ",
       "we", " ", "want", " ", "the", " ", "same", " ", "symplectic", " ", 
