   RowBox[{
    RowBox[{"V", "=", 
     RowBox[{
      RowBox[{"Transpose", "[", "S0", "]"}], ".", 
      RowBox[{"DiagonalMatrix", "[", 
       RowBox[{"Flatten", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Map", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"{", 
