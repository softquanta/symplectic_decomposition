            RowBox[{"RandomReal", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", "1"}], "]"}]}], 
           "]"}], ",", 
          RowBox[{"I", " ", 
           RowBox[{"Map", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"#", ",", "#"}], "}"}], "&"}], ",", 
             RowBox[{"RandomReal", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", "1"}], "]"}]}], 
            "]"}]}], ",", 
          RowBox[{
           RowBox[{"Map", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"#", ",", "#"}], "}"}], "&"}], ",", 
             RowBox[{"RandomReal", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
               RowBox[{"d", "-", "2"}]}], "]"}]}], "]"}], "+", 
           RowBox[{"I", " ", 
            RowBox[{"Map", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"{", 
                RowBox[{"#", ",", "#"}], "}"}], "&"}], ",", 
              RowBox[{"RandomReal", "[", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
                RowBox[{"d", "-", "2"}]}], "]"}]}], "]"}]}]}]}], "}"}], "]"}],
        "]"}], ".", "S0"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"V", "//", "MatrixForm"}], "\[IndentingNewLine]", 
   "\"\<Symplectic eigenvalues include purely imaginary values, purely real \
